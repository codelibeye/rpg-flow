{
  "name": "rpg-flow",
  "version": "1.0.0",
  "description": "TypeScript + WebAssembly Node Tree Library",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc && wasm-pack build src/wasm --target nodejs --out-dir ../dist/wasm",
    "build:wasm": "wasm-pack build src/wasm --target nodejs --out-dir ../dist/wasm",
    "build:ts": "tsc",
    "dev": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "clean": "rm -rf dist node_modules",
    "prepublishOnly": "npm run build",
    "example": "npx ts-node examples/basic-usage.ts",
    "start": "npm run example"
  },
  "keywords": [
    "typescript",
    "webassembly",
    "wasm",
    "node-tree",
    "tree-structure",
    "rpg"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.8",
    "@types/node": "^20.8.10",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2",
    "wasm-pack": "^0.12.1"
  },
  "dependencies": {},
  "files": [
    "dist/**/*",
    "README.md"
  ]
}
